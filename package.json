{
  "name": "business-data-manager",
  "version": "0.0.1",
  "description": "A lightweight, flexible business data management solution",
  "main": "app.js",
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "start": "node app.js",
    "test": "eslint . && mocha --reporter spec --timeout 5000 --recursive",
    "lint": "eslint .",
    "setup-cleanup": "rm -rf public/base && rm -rf views/base",
    "setup-public": "mkdir public/base && cp -r node_modules/govuk_template_jinja/assets/* public/base && cp -rf node_modules/govuk_frontend_toolkit/images/* public/base/images",
    "setup-views": "mkdir views/base && cp node_modules/govuk_template_jinja/views/layouts/* views/base/",
    "build": "npm run setup-cleanup && npm run setup-public && npm run setup-views"
  },
  "repository": {
    "type": "git",
    "url": "git+https://g/Users/goswamih/projects/business-data-manager/models/datasets.jsithub.com/ukhomeoffice/business-data-manager.git"
  },
  "keywords": [
    "business",
    "data",
    "management",
    "manager"
  ],
  "author": "Aker Systems Limited",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukhomeoffice/business-data-manager/issues"
  },
  "homepage": "https://github.com/ukhomeoffice/business-data-manager#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-pg-simple": "^5.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk_frontend_toolkit": "^7.5.0",
    "govuk_template_jinja": "^0.24.1",
    "lusca": "^1.6.0",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "nunjucks": "^3.1.3",
    "passport": "^0.4.0",
    "pg": "^7.4.3",
    "winston": "^3.0.0",
    "eslint": "^5.15.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.15.0",
    "eslint-config-homeoffice": "^2.2.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "fstream": "^1.0.12",
    "js-yaml": "^3.13.1",
    "jsdoc": "^3.5.5",
    "marked": "^0.6.2",
    "mocha": "^5.2.0",
    "node-gyp": "^4.0.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.7"
  }
}
