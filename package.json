{
  "name": "business-data-manager",
  "version": "0.0.1",
  "description": "A lightweight, flexible business data management solution",
  "main": "app.js",
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "start": "node --max-http-header-size=256000 --max-old-space-size=4096 app.js",
    "start-dev": "nodemon --max-http-header-size=256000 --inspect=9239 app.js",
    "test": "eslint . && mocha --reporter spec --timeout 5000 --recursive",
    "lint": "eslint .",
    "setup-cleanup": "rm -rf public/base && rm -rf views/base",
    "setup-public": "mkdir public/base && cp -r node_modules/govuk_template_jinja/assets/* public/base && cp -rf node_modules/govuk_frontend_toolkit/images/* public/base/images",
    "setup-views": "mkdir views/base && cp node_modules/govuk_template_jinja/views/layouts/* views/base/",
    "build": "npm run setup-cleanup && npm run setup-public && npm run setup-views"
  },
  "repository": {
    "type": "git",
    "url": "git+https://g/Users/goswamih/projects/business-data-manager/models/datasets.jsithub.com/ukhomeoffice/business-data-manager.git"
  },
  "keywords": [
    "business",
    "data",
    "management",
    "manager"
  ],
  "author": "Aker Systems Limited",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukhomeoffice/business-data-manager/issues"
  },
  "homepage": "https://github.com/ukhomeoffice/business-data-manager#readme",
  "dependencies": {
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "connect-pg-simple": "^7.0.0-0",
    "crc-32": "^1.2.2",
    "errorhandler": "^1.5.1",
    "express": "^4.21.0",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "govuk_frontend_toolkit": "^9.0.0",
    "govuk_template_jinja": "^0.26.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.4.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-sass-middleware": "^1.0.1",
    "nunjucks": "^3.2.3",
    "passport": "0.6.0",
    "pg": "^8.6.0",
    "role-acl": "^4.5.4",
    "setup": "^0.0.3",
    "start": "^5.1.0",
    "uglify-js": "^3.15.4",
    "winston": "^3.3.3",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "braces": "^3.0.3",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fstream": "^1.0.12",
    "js-yaml": "^4.1.0",
    "jsdoc": "^3.6.7",
    "marked": "^4.0.15",
    "mocha": "^10.7.0",
    "node-gyp": "^9.4.1",
    "node-pre-gyp": "^0.17.0",
    "proxyquire": "^2.1.3",
    "sinon": "^16.1.2"
  }
}
